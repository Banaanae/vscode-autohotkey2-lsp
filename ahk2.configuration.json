{
	"comments": {
		"lineComment": ";",
		"blockComment": [ "/*", "*/" ]
	},
	"brackets": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["%", "%"]
	],
	"autoClosingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["%", "%"],
		["\"", "\""],
		["'", "'"],
		{
			"open": "/*",
			"close": "\n\n*/",
			"notIn": [
				"string"
			]
		}
	],
	"autoCloseBefore": ":.,}])>'\" \n\t",
	"surroundingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["\"", "\""],
		["'", "'"],
		["%", "%"]
	],
	"folding": {
		"markers": {
			"start": "^\\s*\\;\\s*region\\b",
			"end": "^\\s*\\;\\s*endregion\\b"
		}
	}
}
